
<main class="m-4">
    <div class="col-12">
        {{#with navigation}}
        <div class="card m-0">
            <div class="col-12 mb-5">
               <ul class="nav">
                   {{#each links}}
                    <li class="nav-item">
                         <a href="https://github.com/" class="nav-link active">Home</a>
                    </li> 
                    <li class="nav-item">
                            <a href="https://www.google.com/" class="nav-link active">Contacts</a>
                    </li>
                        <li class="nav-item">
                            <a href="https://www.youtube.com/" class="nav-link disabled">Help</a>
                        </li> 
                     {{/each}}     
                 </ul>     
            </div>
        </div>
        {{/with}} 
    </div>
</main>

 <main class="p-5">
    <div class="rov g-2 mt-5">
        {{#with newsBlok}}
         <h3 class="text-center">{{title}}</h3>
          {{#each cards}} 
             <div class="card col-3 text-bg-secondary  g-1">
                {{#if isTop}}
                    <div class="bange text-success rounded-pill text-uppercase"></div>
                {{/if}} 
                {{#if isNew}}   
                   <div class="bange text-success rounded-pill text-uppercase"></div>
                {{/if}}
                <h5>{{title}}</h5>
                <p>{{description}}</p>
             </div>
           {{/each}}
        {{/with}}
    </div>
 </main>    


