<main  class="p-5">
	 {{#with navigation}} 
    <div class="row g-2">
        <div class="col-12">
            <div class="col-12 mb-3">
               <div class="nav nav-tabs">
                 {{#each links}}
                    <div class="nav-item">
                         <a href={{href}} class="nav-link active"> {{text}} </a>
                   </div>
                   {{/each}}     
                 </div>  
            </div>
        </div>
     </div>    
    {{/with}}



   <div class="row">
      {{#each breadcrumb}}
            <div class="col-4 mb-4 mt-4">
				<ul class="nav">
               <li class="nav nav-tabs">
					<a href={{url}} class="nav-link active"> {{name}} </a>
			   </li>
			   </ul>
			</div>	
     {{/each}}
	</div>


    <h1 class="mb-5 mt-4">{{header}}</h1>
	

	<div class="row g-2">
      {{#each sortBlock}}
	   <div class="col-4 mt-4 mb-5">
         <select class="form-select">
			<option selected>{{placeholder}}</option>
                {{#each options}}
                    <option value="{{value}}">{{text}}</option>
                {{/each}}
        </select> 
		</div>   
    {{/each}}	
	

<div class="row g-5"> 
{{#with filterBlock}}
	<div class="card col-4">			
        {{#each sellers.list}}
				  <p>{{title}}</p>
				    <label class="form-check-label">{{option.[0].value}}</label>
					<input name="food" type="checkbox" class="form-check-input">	
		{{/each}}  

		 {{#each brands}}
		   <p>{{title}}</p>
				 <div class="col-8 mb-2">
					<div class="form-check form-switch">
						<label class="form-check-label">{{option.value}}</label>
						<input name="food" type="checkbox" class="form-check-input">
					</div>
				</div>
		 {{/each}}

		{{#each price}}
           <p>{{title}}</p>
		   <input class="form-control">
		   <br>
		   <input class="form-control">
		   <br>
		   <btn class="btn btn-outline-dark" href="{{button.link}}">{{button.text}}</btn>
		{{/each}}


		{{#each filterList}}
			<p>{{title}}</p>
			<div class="form-check">
				{{#each options}} 
					<label class="form-check-label">{{this}}</label>
					<input name="food" type="checkbox" class="form-check-input">
				{{/each}}
			</div>
		{{/each}}
		</div>
{{/with}}




		{{#each goodsList}}
		 <section class="col-4 mb-5">
            <div class="card list-group">
				<img class="card-img-top" src="{{img}}">
				<span class="badge rounded-pill text-uppercose {{#if isHot}}text-bg-warning{{/if}} {{#if isNew}}text-bg-info{{/if}} {{#if isFast}}text-bg-success{{/if}}{{#if isFree}}text-bg-dark{{/if}}">{{text}}</span>
				<h4>{{name}}</h4>
				<p>{{amount}} {{text}}</p>
				<h4>{{price}}</h4>
				{{#each  buttons}}
				 <div class="btn">
						<a href="{{link}}"class="btn {{#if isPrimary}}btn-primary{{/if}} {{#if isSecondary}} btn-secondary{{/if}}">{{text}}</a>
				 </div>
				{{/each}}
				{{#each characteristics}}
				 <ul>
					<li>
						<a  class="nav-link active"> {{title}} : {{value}} </a>
					</li>
				 </ul>
				{{/each}}
		  	</div>
	     </section>
		{{/each}}
		</div>
	 </section>
</div>
   




  <hr>
   <div class="row">
	<div class="col-12 mb-4">
  {{#with service}}
  <h1>{{title}}</h1>
  <p>{{description}}</p>
  {{#each button}}
     <button class="btn btn-primary">
        <span class="text-uppercase"> button primary</span>
    </button>
  {{/each}}
  {{/with}}
	</div>
   </div>



 <div class="row">
	<div class="col-12 mb-4">
     {{#with  footer}}
     <table class="table table-striped table-brodered text-bg-success">
         <a href="{{link}}">{{text}}</a>
     </table>
	 {{/with}}
	 </div>
</div>

</main>



